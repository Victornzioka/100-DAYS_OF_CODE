    "#Write your code below this line ðŸ‘‡\n",
    "def prime_checker(number):\n",
    "    is_prime = True\n",
    "    for i in range(2, number):\n",
    "        if number % i == 0:\n",
    "            is_prime = False\n",
    "\n",
    "    if is_prime:\n",
    "        print(\"Its a prime number\")\n",
    "    else:\n",
    "        print(\"It is not a prime number\")\n"
   ]
